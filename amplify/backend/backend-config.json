{
  "auth": {
    "userPoolGroups": {
      "service": "Cognito-UserPool-Groups",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "auth",
          "resourceName": "xiraAuth",
          "attributes": [
            "UserPoolId",
            "AppClientIDWeb",
            "AppClientID",
            "IdentityPoolId"
          ]
        }
      ]
    },
    "xiraAuth": {
      "service": "Cognito",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "xiraAuthPostConfirmation",
          "triggerProvider": "Cognito",
          "attributes": [
            "Arn",
            "Name"
          ]
        }
      ],
      "customAuth": false
    }
  },
  "api": {
    "xira": {
      "service": "AppSync",
      "providerPlugin": "awscloudformation",
      "output": {
        "authConfig": {
          "defaultAuthentication": {
            "authenticationType": "AMAZON_COGNITO_USER_POOLS",
            "userPoolConfig": {
              "userPoolId": "authxiraAuth"
            }
          },
          "additionalAuthenticationProviders": []
        }
      }
    },
    "AdminQueries": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "auth",
          "resourceName": "xiraAuth",
          "attributes": [
            "UserPoolId"
          ]
        },
        {
          "category": "function",
          "resourceName": "AdminQueriesa02ab228",
          "attributes": [
            "Arn",
            "Name"
          ]
        }
      ]
    }
  },
  "function": {
    "xiraAuthPostConfirmation": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    },
    "AdminQueriesa02ab228": {
      "service": "Lambda",
      "providerPlugin": "awscloudformation",
      "build": true,
      "dependsOn": [
        {
          "category": "auth",
          "resourceName": "xiraAuth",
          "attributes": [
            "UserPoolId"
          ]
        }
      ]
    }
  }
}